<?xml version='1.0' encoding='utf-8'?>
<widget id="com.afadinsro.PinMe" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>PinMe</name>
    <description>
        Asimple application that pins the location of the user on a Google map.
    </description>
    <author email="mills.josef@gmail.com" href="https://millsjoseph.wordpress.com/">
        Adino Concepts
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
